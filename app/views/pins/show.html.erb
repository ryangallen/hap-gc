<form><div>
    <h3><%= link_to @pin.name, @pin %></h3>
    <h4><%= @pin.address %></h4>
    <p><%= @pin.latitude %>, <%= @pin.longitude %></p>
    <h4><%= @pin.date.strftime("%B %d, %Y") %> <%= @pin.time.strftime("%H:%M") %></h4>
    <p><%= @pin.desc %></p>
    <h5>Pinned by: <%= @pin.user.profile_name %></h5>
</div>
<div>
	<p>
		<%= link_to 'Edit', edit_pin_path(@pin) %> |
		<%= link_to 'Continue', root_url %>
	</p>
</div>
</form>